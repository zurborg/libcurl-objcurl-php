{
   "archive" : {
      "exclude" : [
         ".*",
         "*~",
         "vendor/",
         "docs/",
         "Makefile",
         "composer.lock",
         "composer.phar",
         "composer.yaml"
      ]
   },
   "authors" : [
      {
         "email" : "zurborg@cpan.org",
         "homepage" : "https://zurborg.github.io/",
         "name" : "David Zurborg"
      }
   ],
   "autoload" : {
      "psr-0" : {
         "Curl" : "src/"
      }
   },
   "description" : "Object-orientated cURL class for PHP",
   "homepage" : "https://github.com/zurborg/libcurl-objcurl-php",
   "keywords" : [
      "curl"
   ],
   "license" : "MIT",
   "minimum-stability" : "stable",
   "name" : "zurborg/curl-objcurl",
   "require" : {
      "ext-curl" : "*",
      "ext-dom" : "*",
      "ext-iconv" : "*",
      "ext-intl" : "*",
      "ext-openssl" : "*",
      "ext-xml" : "*",
      "php" : "^7.4",
      "psr/log" : "^1.1",
      "sabre/uri" : "*",
      "zurborg/pirate-hooray" : "^2.0",
      "zurborg/wrap-json" : "^1.0"
   },
   "require-dev" : {
      "phpunit/phpunit" : "~9",
      "squizlabs/php_codesniffer" : "~3"
   },
   "type" : "library"
}
